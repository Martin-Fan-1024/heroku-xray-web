{
    "inbounds": [
        {
            "listen": "/etc/caddy/vless", 
            "protocol": "vless", 
            "settings": {
                "clients": [
                    {
                        "id": "$UUID"
                    }
                ], 
                "decryption": "none"
            }, 
            "streamSettings": {
                "network": "ws", 
                "wsSettings": {
                    "path": "/api"
                }
            }
        },
        {
      "port": 2002,
      "listen": "127.0.0.1",
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "$UUID" 
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "grpc",
        "grpcSettings": {
          "serviceName": "api-grpc" 
        }
      }
    }
    ], 
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ]
}
